	"description": "Yii2-article is a module offering basic CMS-functionality",
	"keywords": ["yii2", "article", "cms", "behavior", "widget", "bundle", "action"],

	"license": "MIT",
	"authors": [
		{
			"name": "Pascal Mueller",
			"email": "pascal.mueller@asinfotrack.ch",
			"homepage": "http://www.asinfotrack.ch"
